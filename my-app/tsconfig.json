{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "declarations.d.ts", // Add this line to include your declarations
    "app/(tabs)/index.tsx",
    "app/(tabs)/LocationTracker.jsx"
  ]
}
